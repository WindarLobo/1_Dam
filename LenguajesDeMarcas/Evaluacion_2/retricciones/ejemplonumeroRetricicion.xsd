<xs:element name="numeros">
    <xs:complexType>
      <xs:sequence>
      <xs:element name="numero1"> 
      <xs:simpleType>
<xs:restriction base="xs:decimal">
<xs:enumeration value="1.6"/>
<xs:enumeration value="2.7"/>
<xs:enumeration value="3.6"/>
<xs:enumeration value="4.8"/>
<xs:enumeration value="5.5"/>
</xs:restriction>
</xs:simpleType>
</xs:element>

  <xs:element name="numero2"> 
<xs:simpleType>
<xs:restriction base="xs:decimal">
<xs:fractionDigits value="1"/>
</xs:restriction>
</xs:simpleType>
</xs:element>

 <xs:element name="numero3"> 
<xs:simpleType>
<xs:restriction base="xs:integer">
<xs:minInclusive value="-1"/>
<xs:maxInclusive value="1"/>
</xs:restriction>
</xs:simpleType>
</xs:element>


 <xs:element name="numero4"> 
<xs:simpleType>
<xs:restriction base="xs:integer">
<xs:pattern value="(0-9){5}"/>

</xs:restriction>
</xs:simpleType>
</xs:element>



</xs:sequence>
</xs:complexType>
</xs:element>
</xs:schema>




      
      
      